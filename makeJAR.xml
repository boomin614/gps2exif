<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project default="create_run_jar" name="Create Runnable Jar for Project GPS2EXIF">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="./dest/gps2exif.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="com.cons.gps2exif.Gps2exifMain"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="./WEB-INF/classes"/>
            <zipfileset excludes="META-INF/*.SF" src="./WEB-INF/lib/commons-cli-1.3.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./WEB-INF/lib/commons-compress-1.9.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./WEB-INF/lib/commons-io-2.4.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./WEB-INF/lib/commons-lang3-3.4.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./WEB-INF/lib/commons-imaging-1.0.jar"/>
        </jar>
    </target>
</project>
